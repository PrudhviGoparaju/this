    	  public A1 () {
	System.out.println("A1");
     }
}
	class B1 extends A1 {
		public B1 () {
			System.out.println("B1");
		}
	}		
		class C1 extends B1 {
				public C1() {	
				}
			}
			
public class This {

	public static void main(String[] args) {
		C1 obj = new C1 (); //A1 then C1 
		System.out.println(obj);
	

	}

}

